{
  "version": 2,
  "installCommand": "npm install",
  "buildCommand": "npm run build", 
  "functions": {
    "dist/main.js": { 
      "runtime": "@vercel/node@3.0.0"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)", 
      "dest": "dist/main.js", 
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH"
      ]
    }
  ]
}